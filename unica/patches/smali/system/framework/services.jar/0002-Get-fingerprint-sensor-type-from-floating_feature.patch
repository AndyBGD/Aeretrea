     const-string/jumbo v5, "side"
 
     invoke-virtual {v0, v5}, Ljava/lang/String;->contains(Ljava/lang/CharSequence;)Z
 
-    move-result v0
-
-    if-eqz v0, :cond_4
+    move-result v5
 
-    move v0, v3
-
-    goto :goto_4
-
-    :cond_4
-    move v0, v4
-
-    :goto_4
