+
+    move v0, v3
+
+    goto :goto_4
+
+    :cond_4
+    move v0, v4
+
+    :goto_4
+    sput-boolean v0, Lcom/android/server/biometrics/SemBiometricFeature;->FP_FEATURE_FAKE_AOD:Z
 
     sput-boolean v2, Lcom/android/server/biometrics/SemBiometricFeature;->FP_FEATURE_TSP_BLOCK:Z
 
